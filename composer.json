{
    "name": "jp3cki/yii2-jp-postalcode-validator",
    "type": "yii2-extension",
    "description": "Postal code validator for Yii Framework2",
    "keywords": ["yii2", "extension", "validator", "postalcode", "zipcode", "japan"],
    "license": "MIT",
    "authors": [
        {
            "name": "AIZAWA Hina",
            "email": "hina@fetus.jp"
        }
    ],
    "require": {
        "php":  ">= 8.1.0",
        "ext-pcre": "*",
        "yiisoft/yii2": "~2.0.0"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0.0",
        "jp3cki/coding-standard": "^1.0.0",
        "php-curl-class/php-curl-class": "^9.18.2",
        "phpcompatibility/php-compatibility": "^9.3.5",
        "phpunit/phpunit": "*",
        "proget-hq/phpstan-yii2": "*",
        "squizlabs/php_codesniffer": "^3.8.0"
    },
    "autoload": {
        "psr-4": {
            "jp3cki\\yii2\\jppostalcode\\": "src",
            "jp3cki\\yii2\\jppostalcode\\test\\": "test/src"
        }
    },
    "extra": {
        "bootstrap": "jp3cki\\yii2\\jppostalcode\\internal\\PostalCodeBootstrap"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "yiisoft/yii2-composer": true
        },
        "platform": {
            "php": "8.1.26"
        },
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
