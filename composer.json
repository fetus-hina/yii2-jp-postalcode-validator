{
    "name": "jp3cki/yii2-jp-postalcode-validator",
    "description": "Postal code validator for Yii Framework2",
    "license": "MIT",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "extension",
        "validator",
        "postalcode",
        "zipcode",
        "japan"
    ],
    "authors": [
        {
            "name": "AIZAWA Hina",
            "email": "hina@fetus.jp"
        }
    ],
    "require": {
        "php": ">= 8.1.0",
        "ext-pcre": "*",
        "yiisoft/yii2": "~2.0.51"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0.0",
        "ergebnis/composer-normalize": "^2.45",
        "jp3cki/coding-standard": "^1.0.1",
        "php-curl-class/php-curl-class": "^11.0.0",
        "phpcompatibility/php-compatibility": "^9.3.5",
        "phpunit/phpunit": "^10.5.40",
        "proget-hq/phpstan-yii2": "~0.8.0",
        "squizlabs/php_codesniffer": "^3.11.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "jp3cki\\yii2\\jppostalcode\\": "src",
            "jp3cki\\yii2\\jppostalcode\\test\\": "test/src"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true,
            "yiisoft/yii2-composer": true
        },
        "platform": {
            "php": "8.1.26"
        },
        "sort-packages": true
    },
    "extra": {
        "bootstrap": "jp3cki\\yii2\\jppostalcode\\internal\\PostalCodeBootstrap"
    }
}
