{
    "name": "jp3cki/yii2-jp-postalcode-validator",
    "type": "yii2-extension",
    "description": "Postal code validator for Yii Framework2",
    "keywords": ["yii2", "extension", "validator", "postalcode", "zipcode", "japan"],
    "license": "MIT",
    "authors": [
        {
            "name": "AIZAWA Hina",
            "email": "hina@fetus.jp"
        }
    ],
    "require": {
        "php":  ">= 7.1.0",
        "ext-pcre": "*",
        "yiisoft/yii2": "^2.0.0"
    },
    "require-dev": {
        "php-curl-class/php-curl-class": "^9.1.0",
        "phpunit/phpunit": "*",
        "squizlabs/php_codesniffer": "^3.6.0"
    },
    "autoload": {
        "psr-4": {
            "jp3cki\\yii2\\jppostalcode\\": "src",
            "jp3cki\\yii2\\jppostalcode\\test\\": "test/src"
        }
    },
    "extra": {
        "bootstrap": "jp3cki\\yii2\\jppostalcode\\internal\\Bootstrap"
    },
    "config": {
        "platform": {
            "php": "7.1.33"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
