{
    "name": "jp3cki/yii2-jp-postalcode-validator",
    "type": "yii2-extension",
    "description": "Postal code validator for Yii Framework2",
    "keywords": ["yii2", "extension", "validator", "postalcode", "zipcode", "japan"],
    "license": "MIT",
    "authors": [
        {
            "name": "AIZAWA Hina",
            "email": "hina@fetus.jp"
        }
    ],
    "require": {
        "php":  ">= 7.2.0",
        "ext-pcre": "*",
        "yiisoft/yii2": "^2.0.0"
    },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "php-curl-class/php-curl-class": "^9.1.0",
        "phpcompatibility/php-compatibility": "^9.3",
        "phpunit/phpunit": "*",
        "proget-hq/phpstan-yii2": "*",
        "slevomat/coding-standard": "^8.6",
        "squizlabs/php_codesniffer": "^3.6.1"
    },
    "autoload": {
        "psr-4": {
            "jp3cki\\yii2\\jppostalcode\\": "src",
            "jp3cki\\yii2\\jppostalcode\\test\\": "test/src"
        }
    },
    "extra": {
        "bootstrap": "jp3cki\\yii2\\jppostalcode\\internal\\PostalCodeBootstrap"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "yiisoft/yii2-composer": true
        },
        "platform": {
            "php": "7.2.34"
        },
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
